<Window x:Class="Branchenverzeichnis.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Branchenverzeichnis"
        mc:Ignorable="d"
        Title="MainWindow" Height="485" Width="1200">
    <Grid>
        <StackPanel Orientation="Horizontal" Height="35" VerticalAlignment="Top">
            <Button Content="Unternehmen" Click="OpenCompanyPage_Click" />
            <Button Content="Stammdaten" Click="OpenMasterData_Click" />
        </StackPanel>
        <Frame x:Name="MainFrame" Margin="0,35,0,0" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
